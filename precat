#include<iostream>
    using namespace std;
    int main()
    {
        int num1,num2;

        cout<<"enter first no = ";
        cin>>num1;

        cout<<"enter second no =";
        cin>>num2;

        cout<<"add  = " <<num1+num2;
        cout<<"sub = "<<num1-num2;
        cout<<"div = "<<num1/num2;
        cout<<"multi = "<<num1*num2; 
        


       return 0; 
    }
